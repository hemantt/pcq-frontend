#!groovy

@Library("Infrastructure")

def type = "nodejs"
def product = "pcq"
def component = "frontend"

withPipeline(type, product, component) {

  after('build') {
    sh 'yarn setup'
  }

  installCharts()
  enableAksStagingDeployment()
  disableLegacyDeployment()
  // enableSlackNotifications('#pcq-builds')

}
